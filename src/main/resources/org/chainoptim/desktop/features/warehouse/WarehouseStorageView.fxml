<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="org.chainoptim.desktop.features.warehouse.controller.WarehouseStorageController"
      prefHeight="400.0" prefWidth="600.0" style="-fx-padding: 20px;" spacing="10">

    <HBox alignment="CENTER_LEFT">
        <Label text="Compartments" styleClass="tab-title"/>
        <Region HBox.hgrow="ALWAYS"/>
        <Button text="Add Compartment" onAction="#handleAddCompartment" styleClass="standard-write-button"/>
    </HBox>
    <VBox fx:id="compartmentsVBox" style="-fx-padding: 10px;"/>
</VBox>
